% Libertine/Libertinus font setup (all engines)
\newcommand{\llncscrypto@libertine}{%
	\ifLuaTeX
		% LuaTeX: Libertinus with unicode-math
		\typeout{llncscrypto: Libertinus setup for LuaTeX}
		\RequirePackage{amssymb}
		\RequirePackage{mathtools}
		\RequirePackage{fontspec}
		\RequirePackage{lualatex-math}
		\RequirePackage{moresize}
		\RequirePackage{nicefrac}
		\RequirePackage{siunitx}
		\RequirePackage{unicode-math}
		\setmainfont[
			SmallCapsFeatures={RawFeature=+smcp},
			RawFeature={+calt,+case,+cpsp,+kern,+liga,+lnum,+mark,+mkmk,+pnum,+ss06},
			ItalicFont = LibertinusSerif-Italic.otf,
			BoldFont = LibertinusSerif-Bold.otf,
			BoldItalicFont = LibertinusSerif-BoldItalic.otf
		]{LibertinusSerif-Regular.otf}
		\setmathfont[
			AutoFakeBold,
			Scale=MatchUppercase,
			RawFeature={+calt,+case,+kern,+liga}
		]{LibertinusMath-Regular.otf}
		\setmonofont[
			Scale=MatchLowercase,
			SmallCapsFeatures={RawFeature=+smcp},
			RawFeature={+calt,+case,+dlig,+kern,+liga,+lnum,+mark,+mkmk,+pnum}
		]{notosansmono}
		\setsansfont[
			Scale=MatchLowercase,
			SmallCapsFeatures={RawFeature=+smcp},
			RawFeature={+calt,+case,+kern,+liga,+lnum,+mark,+mkmk,+pnum}
		]{ysabeau}
		% Remove math font clashes
		\let\mathbfcal\relax
		\let\mathbfscr\relax
		\RequirePackage{mathrsfs} % \mathscr
		\RequirePackage[cal=stixplain,bb=bboldx,scr=esstix]{mathalpha}
	\else
		\ifXeTeX
			% XeTeX: Libertinus with unicode-math
			\typeout{llncscrypto: Libertinus setup for XeTeX}
			\RequirePackage{amssymb}
			\RequirePackage{mathtools}
			\RequirePackage{moresize}
			\RequirePackage{nicefrac}
			\RequirePackage{siunitx}
			\RequirePackage{fontspec}
			\RequirePackage{unicode-math}
			\setmainfont[
				SmallCapsFeatures={RawFeature=+smcp},
				RawFeature={+calt,+case,+cpsp,+kern,+liga,+lnum,+mark,+mkmk,+pnum,+ss06},
				ItalicFont = LibertinusSerif-Italic.otf,
				BoldFont = LibertinusSerif-Bold.otf,
				BoldItalicFont = LibertinusSerif-BoldItalic.otf
			]{LibertinusSerif-Regular.otf}
			\setmathfont[
				AutoFakeBold,
				Scale=MatchUppercase,
				SmallCapsFeatures={RawFeature=+smcp},
				RawFeature={+calt,+case,+kern,+liga}
			]{LibertinusMath-Regular.otf}
			\setmonofont[
				Scale=MatchLowercase,
				SmallCapsFeatures={RawFeature=+smcp},
				RawFeature={+calt,+case,+dlig,+kern,+liga,+lnum,+mark,+mkmk,+pnum}
			]{inconsolata}
			\setsansfont[
				Scale=MatchLowercase,
				SmallCapsFeatures={RawFeature=+smcp},
				RawFeature={+calt,+case,+kern,+liga,+lnum,+mark,+mkmk,+pnum}
			]{Ysabeau}
			% Remove math font clashes
			\let\mathbfcal\relax
			\let\mathbfscr\relax
			\RequirePackage{mathrsfs} % \mathscr
			\RequirePackage[cal=stixplain,bb=bboldx,scr=esstix]{mathalpha}
		\else
			% pdfLaTeX or other: fallback to Type1 Libertine and newtxmath
			\typeout{llncscrypto: Libertine/Libertinus OTF not supported for this engine.}
			\RequirePackage[T1]{fontenc}
			\RequirePackage{amssymb}
			\RequirePackage{mathtools}
			\RequirePackage{moresize}
			\RequirePackage{nicefrac}
			\RequirePackage{siunitx}
			\RequirePackage{libertineRoman}
			\RequirePackage{AnonymousPro}
			\RequirePackage{ysabeau}
			\RequirePackage[libertine]{newtxmath}
			\RequirePackage[bb=bboldx,cal=cm,scr=esstix]{mathalpha}
		\fi
	\fi
}

