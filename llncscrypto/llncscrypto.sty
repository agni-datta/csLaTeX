% Customize backrefs
\renewcommand*{\backref}[1]{}
\renewcommand*{\backrefalt}[4]{%
	\ifcase #1\or (Cited on p.~#2).%
	\else (Cited on pp.~#2).%
	\fi
}
% Full reference combining hyperref, autoref, and nameref.
\newcommand*{\fullref}[1]{\hyperref[{#1}]{\autoref*{#1}~\nameref*{#1}}}

