%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Calculus and Analysis
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Differential operator
\newcommand*{\dd}{\mathrm{d}}
% Differential quotient
\newcommand*{\ddiff}[2]{\frac{\diff{#1}}{\diff{#2}}}
% Partial derivative
\newcommand*{\pdiff}[2]{\frac{\partial{#1}}{\partial{#2}}}
% Gradient
\newcommand*{\gradientd}[1]{\overrightarrow{\triangledown #1}}
% Differential of order n
\newcommand*{\Diff}[1]{\mathrm{d^#1}}
% Definite integral
\newcommand*{\defintegral}[2]{\int_{#1}^{#2}}
% Limits
\newcommand*{\limit}[2]{\lim_{{#1} \to {#2}}}
% Infinite series
\newcommand*{\series}[1]{\sum_{#1}^{\infty}}
% Taylor series
\newcommand*{\taylor}[1]{\sum_{n=0}^{\infty} \frac{{#1^{(n)}}}{n!}(x-a)^n}
% Partial fraction decomposition
\newcommand*{\partialfrac}[2]{\frac{#1}{#2}}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Vectors
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Unit vector i
\renewcommand*{\i}{\hat{i}}
% Unit vector j
\renewcommand*{\j}{\hat{j}}
% Unit vector k
\renewcommand*{\k}{\hat{k}}
% Matrices
\newcommand{\mat}[1]{\mathbf{#1}}
% Vectors
\newcommand{\vect}[1]{\mathbf{#1}}
% Magnitude of a vector
\newcommand{\magnitude}[1]{\left\lVert #1 \right\rVert}
% Vector projection
\newcommand{\projection}[2]{\text{proj}_{#1}#2}
% Vector component orthogonal to another vector
\newcommand{\orthogonal}[2]{\text{orth}_{#1}#2}
% Vector derivative
\newcommand{\vectdiff}[1]{\frac{d\vect{#1}}{dt}}
% Vector integral
\newcommand{\vectintegral}[1]{\int \vect{#1} \, dt}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Combinatorics
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Permutations
\newcommand*{\nPr}[2]{{#1}P{#2}}
% Permutations with factorials
\newcommand*{\nPrf}[2]{\frac{#1!}{(#1-#2)!}}
% Combinations
\newcommand*{\nCr}[2]{{#1}C{#2}}
% Combinations with factorials
\newcommand*{\nCrf}[2]{\frac{#1!}{(#1-#2)!#2!}}
% Binomial coefficient
\newcommand{\binomialcoeff}[2]{\binom{#1}{#2}}
% Multinomial coefficient
\newcommand{\multinomialcoeff}[2]{\binom{#1}{#2_1, #2_2, \ldots, #2_k}}
% Stirling number of the first kind
\newcommand{\stirlingfirst}[2]{\genfrac{\{}{\}}{0pt}{}{#1}{#2}}
% Stirling number of the second kind
\newcommand{\stirlingsecond}[2]{\genfrac{\{}{\}}{0pt}{}{#1}{#2}}

