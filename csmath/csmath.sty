\newcommand{\Q}{\ensuremath{\mathbb{Q}}}
% Set of real numbers
\newcommand{\R}{\ensuremath{\mathbb{R}}}
% Set of complex numbers
\newcommand{\C}{\ensuremath{\mathbb{C}}}
% Set of positive integers
\newcommand{\Zplus}{\ensuremath{\mathbb{Z}^+}}
% Set of negative integers
\newcommand{\Zneg}{\ensuremath{\mathbb{Z}^-}}
% Set of non-negative integers
\newcommand{\Znonneg}{\ensuremath{\mathbb{Z}_0^+}}
% Set of non-positive integers
\newcommand{\Znonpos}{\ensuremath{\mathbb{Z}_0^-}}
% Set of positive real numbers
\newcommand{\Rplus}{\ensuremath{\mathbb{R}^+}}
% Set of non-negative real numbers
\newcommand{\Rnonneg}{\ensuremath{\mathbb{R}_0^+}}
% Set of positive rational numbers
\newcommand{\Qplus}{\ensuremath{\mathbb{Q}^+}}
% Set of negative rational numbers
\newcommand{\Qneg}{\ensuremath{\mathbb{Q}^-}}
% Set of non-negative rational numbers
\newcommand{\Qnonneg}{\ensuremath{\mathbb{Q}_0^+}}
% Set of imaginary numbers
\newcommand{\Imag}{\ensuremath{\mathbb{I}}}
