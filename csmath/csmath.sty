% Declare that this is a LaTeX2e package
\def\filename{csmath}
\def\fileversion{v1}
\def\filedate{2024/04/17}
\def\fileauthor{Agni Datta}

\NeedsTeXFormat{LaTeX2e}
\AtEndOfPackage{\ProvidesPackage{\filename}[\filedate\space\fileversion\space(\fileauthor)]}
\typeout{Package `\filename', \fileversion, <\filedate>}
\ProvidesPackage{csmath}[2024/03/25 Custom LaTeX commands for computer scientists]

\makeatletter

% Check if amsmath package is loaded, and if not, load it
\@ifpackageloaded{amsmath}{}{\RequirePackage{amsmath}} % Enhance mathematics features
\@ifpackageloaded{xspace}{}{\RequirePackage{xspace}} % Proper spacing using xspace

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Mathematical Notation
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Ensure that the argument is typeset in math mode
\newcommand{\mathify}[1]{\ensuremath{#1}}
% Format a set with curly braces, with automatic sizing of the braces
\newcommand{\set}[1]{\mathify{\left\{ #1 \right\}}}
% Format the floor function with proper brackets
\newcommand{\floor}[1]{\ensuremath{\left\lfloor #1 \right\rfloor}}
% Format the absolute value with vertical bars and automatic sizing
\newcommand{\abs}[1]{\mathify{\left| #1 \right|}}
% Format the intersection symbol
\newcommand{\intersection}{\ensuremath{\cap}}
% Format the union symbol
\newcommand{\union}{\ensuremath{\cup}}
% Format the XOR (exclusive or) symbol
\newcommand{\xor}{\ensuremath{\oplus}}
% Format the big XOR symbol for summation-like expressions
\newcommand{\Xor}{\ensuremath{\bigoplus}}
% Format the equality by definition symbol
\newcommand{\eqdef}{\ensuremath{\stackrel{\textrm{def}}{=}}}
% Format the uncertain equality symbol (e.g., for conjectured equality)
\newcommand{\eqq}{\ensuremath{\stackrel{\textrm{?}}{=}}}
% Format the uncertain membership symbol (e.g., for conjectured set membership)
\newcommand{\inq}{\ensuremath{\stackrel{\textrm{?}}{\in}}}
% Define a command to format parentheses with automatic sizing
\newcommand{\paren}[1]{\ensuremath{\left(#1\right)}}
% Define a command to convert a symbol to math mode
\newcommand{\convert}[1]{\ensuremath{#1}}
% Define operators for arg min with correct spacing and font
\newcommand{\argmin}{\ensuremath{\mathsf{arg\,min}}}
% Define operators for arg max with correct spacing and font
\newcommand{\argmax}{\ensuremath{\mathsf{arg\,max}}}


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Number Theory
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Format the greatest common divisor command
\renewcommand*{\gcd}{\ensuremath{\mathsf{gcd}}}
% Format the least common multiple command
\newcommand{\lcm}{\ensuremath{\mathsf{lcm}}}
% Format the least common multiple-set command
\newcommand{\lcmset}[1]{\ensuremath{\mathsf{lcm}\left(#1\right)}}
% Format the Jacobi symbol
\newcommand{\jacobi}[2]{\ensuremath{\left(\frac{#1}{#2}\right)}}
% Format the Legendre symbol
\newcommand{\legendre}[2]{\ensuremath{\left(\frac{#1}{#2}\right)}}
% Format the Euler's phi function
\newcommand{\phiEuler}[1]{\ensuremath{\varphi(#1)}}
% Format the Euler's totient function
\newcommand{\totient}[1]{\ensuremath{\phi(#1)}}
% Format the factorial function
\newcommand{\factorial}[1]{\ensuremath{#1!}}
% Format the falling factorial
\newcommand{\fallingfactorial}[2]{\ensuremath{#1^{\underline{#2}}}}
% Format the rising factorial
\newcommand{\risingfactorial}[2]{\ensuremath{#1^{\overline{#2}}}}
% Format the binomial coefficient
\newcommand{\binomial}[2]{\ensuremath{\binom{#1}{#2}}}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Set Theory
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Format the power set
\newcommand{\powerset}[1]{\ensuremath{\mathcal{P}(#1)}}
% Format the cardinality of a set
\newcommand{\cardinality}[1]{\ensuremath{|#1|}}
% Format the set difference
\newcommand{\setdifference}{\ensuremath{\backslash}}
% Format the symmetric difference
\newcommand{\symmetricdifference}{\ensuremath{\triangle}}
% Format the Cartesian product
\newcommand{\cartesianproduct}{\ensuremath{\times}}
% Format the disjoint union
\newcommand{\disjointunion}{\ensuremath{\uplus}}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Game Theory
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Format the strategy set
\newcommand{\strategies}[1]{\ensuremath{S_{#1}}}
% Format the payoff function
\newcommand{\payoff}[1]{\ensuremath{u_{#1}}}
% Format the Nash equilibrium
\newcommand{\nash}{\ensuremath{\text{Nash}}}
% Format the dominant strategy
\newcommand{\dominant}{\ensuremath{\text{Dominant}}}
% Format the dominant strategy
\newcommand{\dominated}{\ensuremath{\text{Dominated}}}
% Format the mixed strategy
\newcommand{\mixed}{\ensuremath{\text{Mixed}}}
% Format the best response
\newcommand{\bestresponse}{\ensuremath{\text{BestResponse}}}
% Format the maximin strategy
\newcommand{\maximin}{\ensuremath{\text{Maximin}}}
% Format the minimax strategy
\newcommand{\minimax}{\ensuremath{\text{Minimax}}}

